{
  "presets": ["react-app"],
  "env": {
    "production": {
      "plugins": [
        ["emotion", { "hoist": true }]
      ]
    },
    "development": {
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }]
      ]
    },
    "test": {
      "plugins": [
        ["emotion", { "autoLabel": true }]
      ]
    }
  }
}
